

<%if (success && success.length){%>
  <div class="alert alert-success alert-dismissible fade show  mt-2 p-2" role="alert">
    <%= success %>
      <button type="button" class="btn-close p-1 m-2" data-bs-dismiss="alert" aria-label="Close"></button>
      </button>
  </div>
  <% } %>
    <%if (error && error.length){%>
      <div class="alert alert-danger alert-dismissible fade show mt-2 p-1" role="alert">
        <%= error %>
          <button type="button" class="btn-close p-2" data-bs-dismiss="alert" aria-label="Close"></button>
          </button>
      </div>
      <% } %>